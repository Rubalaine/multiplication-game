 action = setInterval(()=>{\r\n        timeLeft -=1;\r\n        timerBoard.innerText = ` ${timeLeft} Seconds Left`\r\n        if(timeLeft === 0){\r\n            stopCountdown();\r\n            isPlaying = false;\r\n            timerBoard.innerText = `time is over`;\r\n        }\r\n    }, 1000)\r\n    \r\n}\r\nconst stopCountdown = ()=>{\r\n    clearInterval(action);\r\n}\r\nif(options)\r\noptions.forEach(option =>{\r\n    option.addEventListener('click', event=>{\r\n        event.preventDefault();\r\n        if(isPlaying === true){\r\n            if(option.innerText == correctAnswer){\r\n                score++;\r\n                scoreBoard.innerText = `Score: ${score}`;\r\n                message('right');\r\n                generateQuestion(); \r\n            }\r\n            else{\r\n                message('wrong');\r\n                generateQuestion();\r\n            }\r\n        }\r\n       \r\n    })\r\n})"},"lineCount":null}},"error":null,"hash":"db7beb71ad0efe52c81788ccfa5ae7bb","cacheData":{"env":{}}}